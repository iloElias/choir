{
  "name": "ilias/choir",
  "description": "A PHP HTTP requests handler template",
  "type": "project",
  "version": "1.1.1",
  "license": "MIT",
  "keywords": [
    "PHP",
    "HTTP",
    "requests",
    "handler",
    "template"
  ],
  "homepage": "https://github.com/ilias/choir",
  "autoload": {
    "psr-4": {
      "Ilias\\Choir\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpunit": "./vendor/bin/phpunit tests --colors"
  },
  "authors": [
    {
      "name": "Murilo Elias",
      "email": "murilo7456@gmail.com",
      "homepage": "https://ilias.dev",
      "role": "Developer"
    }
  ],
  "require": {
    "ilias/rhetoric": "^1.0",
    "ilias/opherator": "^1.0",
    "ilias/maestro": "^1.0",
    "ilias/dotenv": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": false
}
